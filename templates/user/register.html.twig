{# Register page #}
{% extends 'base.html.twig' %}
{% block javascripts %}
{{ parent() }}
<script src="{{asset('js/nickCheck.js')}}"></script>
{% endblock %}

{% block title %}{{title}}{% endblock %}

{% block body %}

<body>

    <div class="container mt-4">
        <div class="row">

            <h1 class="m-auto display-4">{{title}}</h1>
        </div>
        <div class="row mt-4">
            <div class="col-md-3">
            </div>
            <div class="col-md-6">
                {{ form_start(form)}}
                {{ form_widget(form)}}
                {{ form_end(form)}}

            </div>
            <div class="col-md-m3">

            </div>
        </div>
    </div>
</body>

{% endblock %}